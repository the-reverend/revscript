/* rev. 2 - minor bug fixes                                                   */

globals=_setglobals(debug)
call _timeout 60
call zocsynctime 500
call on halt name _halt
call _rseed

debug=0
minunknownsinpath=0

if _synchronize()\="command" then call _error "please start at the command prompt."

/* load ini variables */
   load="gname vwname vname mname"
   call _load_ini_variables

reclen=_mapinit(mname)
home=_getinfo("sect")
call zocsend "ctq"

rmask="0PST"
mins=1
maxs=20000
target=0

call _getviews vwname
call _getvoids vname, "file"
stamp=_getservertime()
call _cn9 "spaces"

if debug then call _debug "LOG", "EPROBE SESSION: "||date("N")||", "||time("N")

/* GET EPROBE MODE */
mode=_chooselist("select a probe method:", "RANDOM;UNEXPLORED;FILE", ";", "c")
select
 when mode="RANDOM" then nop
 when mode="UNEXPLORED" then target=_asknum("start at what sector?", 1, "c")-1
 when mode="FILE" then do
  parse value zocgetglobaloption("ScriptPath") with . '="' fpath '"'
  fpath=fpath||"\"
  fname="*.txt"
  fname=zocgetfilename("enter sector list filename:", fpath||fname)
  if fname="##CANCEL##" then call _quit "script cancelled by user."
  if \_fileexists(fname) then call _error fname||" does not exist."
  if \_fileopenr(fname) then call _error "unable to open file "||fname||"."
 end /*when*/
end /*select*/

/* GET REPORT MODE */
rmode=_chooselist("select a report mode:", "SUBSPACE;FILE;BOTH;NONE", ";", "c")
if rmode="FILE" | rmode="BOTH" then do
 parse value zocgetglobaloption("DownloadAltPath") with . '="' rpath '"'
 rpath=rpath||"\"
 rname=gname||"_eprobe_report.txt"
 rname=zocask("enter report filename:", rname)
 if rname="##CANCEL##" then call _quit "script cancelled by user."
 if pos(".",rname)=0 then rname=rname||".txt"
 rname=rpath||rname
end /*if*/
else rname=""
select
 when rmode="SUBSPACE" then rmode="sub"
 when rmode="FILE" then rmode="file"
 when rmode="BOTH" then rmode="sub;file"
 when rmode="NONE" then rmode=""
end /*select*/
if debug then call _debug "LOG", "mode:"||mode||" report:"||rmode

/* MISCELLANEOUS MODES */
if mode="UNEXPLORED" then unvisited=1
else unvisited=_yesno("check computer for unvisited sectors?")
autobuy=_yesno("autobuy eprobes?")
autovoid=_yesno("autovoid where probes are destroyed?")
if debug then call _debug "LOG", "unvisited:"||unvisited
if debug then call _debug "LOG", "autobuy:"||autobuy
if debug then call _debug "LOG", "autovoid:"||autovoid

/* GET MASK (disabled) */
if 0 & rmode\="" then do
 call _newline _ansi("bt;fwh")
 call zocwriteln "the report mask tells the script which items to report."
 call zocwriteln "  0=class 0 ports   N=navhaz"
 call zocwriteln "  B=beacon          P=planets"
 call zocwriteln "  F=fighters        S=ships"
 call zocwriteln "  M=mines           T=traders"
 call zocwriteln "      *=ALL OF THE ABOVE"
 rmask=zocask("report mask?", rmask)
 if rmask="##CANCEL##" then call _quit "script cancelled by user"
 rmask=lower(rmask)
end /*if*/
if debug then call _debug "LOG", "mask:"||rmask

if mode="RANDOM" | mode="UNEXPLORED" then do
 call zocsend "v"
 if zocwait(", ports")=640 then call _error "timeout"
 parse value _lastline() with . "sectors " maxs ", ports"
 if zocwait("Command [")=640 then call _error "timeout"
end /*if*/
if debug then call _debug "LOG", "ns:"||maxs

_sl.=0
_ex.=0
target=_getnext(mode,target,fname,mins,maxs)

if pos("file",rmode)>0 then do
 if _fileopenw(rname) then do
  call lineout rname, ""
  call lineout rname, "EPROBE SESSION: "||date("N")||", "||time("N")
  call _fileclose rname
 end /*if*/
end /*if*/

call go_out

done=0
do while \done

 if unvisited & \(msg="##DESTROYED##" & autovoid) then do
  call zocsend "ci"||target||"^Mq"
  if zocwait("<Computer activated>")=640 then call _error "timeout"
  ans=zocwaitmux("has warps to sector(s)", "never visited sector")
  select
   when ans=0 then do
    msg="##SUCCESS##"
    sect=0
   end /*when*/
   when ans=1 then parse value _launchprobe(target, rmask, rmode, rname, stamp) with msg ";" sect
   when ans=640 then call _error "timeout"
   otherwise nop
  end /*select*/
 end /*if*/
 else parse value _launchprobe(target, rmask, rmode, rname, stamp) with msg ";" sect
if debug then call _debug "LOG", "launched:"||target||" "||msg||" "||sect

 select
  when msg="##NO PROBES##" & autobuy then do
   call come_in
   if _buyhardware("e0")=0 then done=1
   call go_out
  end
  when msg="##NO PROBES##" then done=1
  when msg="##NO PATH##" then target=_getnext(mode,target,fname,mins,maxs)
  when msg="##SUCCESS##" then target=_getnext(mode,target,fname,mins,maxs)
  when msg="##DESTROYED##" & autovoid then do
   call zocsend "^^s"||sect||"^Mq"
   _v.sect=1
   if sect=target then do
    target=_getnext(mode,target,fname,mins,maxs)
    msg="##NO PATH##"
   end /*if*/
if debug then call _debug "LOG", "void:"||sect
  end /*when*/
  when msg="##DESTROYED##" then target=_getnext(mode,target,fname,mins,maxs)
  otherwise nop
 end /*select*/

 if target=0 then done=1

end /*while*/

call come_in

exit

_getnext:
 gnm=arg(1)
 gnt=arg(2)
 gnfname=arg(3)
 gnmin=arg(4)
 gnmax=arg(5)
 gnr=0

 do until \_sl.gnr & \_v.gnr & (unknownsinpath >= minunknownsinpath)

  select
   when gnm="UNEXPLORED" then do
    if list="" | list="LIST" then do
     list=_getunexplored(gnt+1)
     if zocwait("Command [")=640 then call _error "timeout"
    end /*if*/
    parse var list gnr ";" list
    if gnr="" then gnr=0
if debug then call _debug "LOG", "unexplored list:"||_dwords(list,";")||" sectors in list"
   end /*when*/
   when gnm="RANDOM" then gnr=random(gnmin, gnmax)
   when gnm="FILE" then do
    if _fileeof(gnfname) then do
     call _fileclose gnfname
     gnr=0
    end /*if*/
    else do
     gnr=strip(linein(gnfname))
     do while _vs.gnr>=stamp & \_fileeof(gnfname) & gnr\=""
      gnr=strip(linein(gnfname))
     end
    end
    if gnr="" then do
     call _fileclose gnfname
     gnr=0
    end /*if*/
   end /*when*/
   otherwise leave
  end /*select*/
  if gnr>0 & minunknownsinpath>0 then do
call zocwriteln "trying" gnr
   gnr=_mapfind(home,"s:"||gnr||";uv:1",mname,reclen)
call zocwriteln gnr _path.home.gnr
   unknownsinpath=0
   if gnr>0 then do
    do i=1 to _dwords(_path.home.gnr,";")
     gns=_dword(_path.home.gnr,i,";")
     if \_sv.gns then unknownsinpath=unknownsinpath+1
    end /*do*/
call zocwriteln gnr unknownsinpath
   end /*if*/
  end /*if*/
 end /*do*/
return gnr

go_out:
  /* leave dock to eprobe from another location */
  /* you might wanna buy ore or something */
return
  if \_twarp(9) then exit
  call zocsend "o16^M^Mpt^M^Mz0^Mz0^M"
return

come_in:
  /* return to dock to buy probes */
return
  if \_twarp(1154) then exit
return

